                "  let ymax = 0.003;",
                "  ",
                "  let A = -6.9 * Math.log(random/15.7);",
                "  ",
                "  let x = (A/25) * xmax;",
                "  let y = (A/25) * ymax;",
                "  ",
                "  if(position < 16 && position >= 0) {",
                "      jibo.animate.setEyePosition(x,0);",
                "    }",
                "    else if(position < 22 && position > 16) {",
                "      jibo.animate.setEyePosition(x,y);",
                "    }",
