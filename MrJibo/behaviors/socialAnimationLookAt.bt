                "      jibo.animate.setEyePosition(-x,0);",
                "    }",
                "    else if(position < 65 && position > 48) {",
                "      jibo.animate.setEyePosition(-x,-y);",
                "    }",
                "    else if(position < 73 && position > 65) {",
                "      jibo.animate.setEyePosition(0,-y);",
                "    }",
                "    else if(position < 93 && position > 73) {",
