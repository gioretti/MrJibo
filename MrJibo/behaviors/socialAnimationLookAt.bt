                "  // create a look-at builder",
                "let builder = animate.createLookatBuilder();",
                "",
                "// configure the builder",
                "builder.setContinuousMode(true);",
                "",
                "// trigger a look-at instance",
                "let target = new animate.THREE.Vector3(1.0, 0.0, 1.0);",
